<?xml version="1.0" encoding="utf-8"?>
<conf>
  <module tag="HeaderFooter" name="页眉页脚">
    <section tag="HeaderStyle" name="页眉">
      <item tag="Fonts">
        <error-msg>
          (3.1.1)页眉字体错误，应该宋体
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          (3.1.2)页眉字号错误，应该小五号
        </error-msg>
      </item>
      <item tag="jc">
        <error-msg>
          (3.1.3)页眉对齐方式设置错误，应该居中对齐
        </error-msg>
      </item>
    </section>
    <section tag="FooterStyle" name="页脚">
      <item tag="Fonts">
        <error-msg>
          (3..1)页脚字体错误，应该宋体
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          (3.2.2)页脚字号错误，应该小五号
        </error-msg>
      </item>
      <item tag="jc">
        <error-msg>
          (3.2.3)页脚对齐方式设置错误，应该居中对齐
        </error-msg>
      </item>
      <item tag="JudgeResult">
        <error-msg>
          (3.2.4)页脚页码设置错误,缺少左右横线
        </error-msg>
      </item>
    </section>
  </module>
</conf>