<?xml version="1.0" encoding="utf-8"?>
<conf>
  <module tag="AbstractStyle" name="摘要">
    <section tag="CNAbstractTitle" name="中文摘要标题">
      <item tag="Fonts">
        <error-msg>
          中文摘要标题字体错误，应为黑体；
        </error-msg>
      </item>
      <item tag="jc">
        <error-msg>
          中文摘要标题未居中；
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          中文摘要标题字号错误，应为小三号；
        </error-msg>
      </item>
    </section>
    <section tag="CNAbstractText" name="中文摘要正文">
      <item tag="Fonts">
        <error-msg>
          中文摘要正文字体错误，应为宋体；
        </error-msg>
      </item>
      <item tag="jc">
        <error-msg>
          中文摘要标题未居中；
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          中文摘要正文字号错误，应为小四号；
        </error-msg>
      </item>
    </section>
    <section tag="CNKeyWords" name="中文摘要关键词">
      <part tag="CNKeyWordsTip" name="中文关键词提示">
        <item tag="Fonts">
          <error-msg>
          中文关键词标题字体错误，应为黑体；
          </error-msg>
        </item>
        <item tag="size">
          <error-msg>
           中文关键词标题字号错误，应为小四号；
          </error-msg>
        </item>
      </part>
      <part tag="CNKeyWordsText" name="中文关键词内容">
        <item tag="Fonts">
          <error-msg>
           中文摘要关键词内容字体错误，应为仿宋_GB2312；
          </error-msg>
        </item>
        <item tag="size">
          <error-msg>
            中文关键词内容字号错误，应为小四号；
          </error-msg>
        </item>
      </part>
    </section>
    <section tag="ENAbstractTitle" name="英文摘要标题">
      <item tag="Fonts">
        <error-msg>
          英文摘要标题提示字体错误，应为Times New Roman；
        </error-msg>
      </item>
      <item tag="jc">
        <error-msg>
          英文摘要标题未居中；
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          英文摘要标题字号错误，应为小三号；
        </error-msg>
      </item>
    </section>
    <section tag="ENAbstractText" name="英文摘要正文">
      <item tag="Fonts">
        <error-msg>
         英文摘要正文提示字体错误，应为Times New Roman；
        </error-msg>
      </item>
      <item tag="jc">
        <error-msg>
          英文摘要正文未居中；
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          英文摘要正文字号错误，应为小四号；
        </error-msg>
      </item>
    </section>
  </module>
</conf>