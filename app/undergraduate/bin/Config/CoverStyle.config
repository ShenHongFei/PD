<?xml version="1.0" encoding="utf-8"?>
<conf>
  <module tag="CoverStyle" name="封面">
    <section tag="Title" name="标题">
      <part tag="Heading" name="论文大标题">
        <item tag="Text">
          <error-msg>
            封面大标题文本错误，应为“大连理工大学本科毕业设计（论文）”；
          </error-msg>
        </item>
      </part>
    </section>
    <section tag="StudentInfo" name="学生信息">
      <item tag="Fonts">
        <error-msg>
          学生信息字体错误，应为宋体；
        </error-msg>
      </item>
      <item tag="size">
        <error-msg>
          学生信息字号错误，应为小三号；
        </error-msg>
      </item>
    </section>
  </module>
</conf>